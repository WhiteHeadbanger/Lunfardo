cheto Saludador
    laburo arranque(mi)
        devolver nada
    chau

    laburo saludar(mi)
        matear("Holis, soy Saludador")
    chau
chau

cheto Hola
    laburo arranque(mi, otro_objeto)
        poneleque mi.otro_objeto = otro_objeto
    chau

    laburo saludar(mi)
        matear(mi.otro_objeto.saludar())
    chau
chau

poneleque s = nuevo Saludador()
poneleque h = nuevo Hola(s)
h.saludar()